<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Kevin Rebakure | Profile</title>
    <link rel="stylesheet" href="output.css" />
  </head>
  <body class="bg-gray-900 text-white">
    <div class="mx-auto w-[800px]">
      <%- include('partials/header.ejs') %>

      <main>
        <section class="p-10">
          <h1 class="p-10 text-center text-4xl font-semibold">My Profile</h1>
          <div class="mx-auto space-y-5 rounded-lg border border-white p-5">
            <p>Names: <%- data.profile.names %></p>
            <p>Occupation: <%- data.profile.occupation %></p>
            <p>Based in: <%- data.profile.location %></p>
            <p>Updated on: <%- data.profile.date %></p>
          </div>
        </section>
      </main>
    </div>
  </body>
</html>
